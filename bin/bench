#!/bin/env php
<?php

require __DIR__.'/../vendor/autoload.php';

(new Seld\AutoloadBench\Runner(dirname(__DIR__).'/build'))
    ->prepare(4000)
    ->run([200, 500, -500], 100)
    ->prepare(4000, 'SharedVendor\\')
    ->run([300], 100)
    ->prepare(4000, 'SharedVendor\\', 2)
    ->run([300], 100)
    ->prepare(4000, '', 2)
    ->run([300], 40)
;
